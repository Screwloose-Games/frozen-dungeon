config_version=5

[application]

config/name="Frozen Dungeon"
run/main_scene="res://ui/main_menu.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
boot_splash/bg_color=Color(0.109804, 0.109804, 0.109804, 1)
boot_splash/image="res://ui/splash_screen/wildjam-splash.png"
config/icon="res://icon.svg"

[audio]

general/default_playback_type.web=0

[autoload]

SceneTransitionManager="*res://ui/scene_transitions/scene_transition_manager.tscn"
